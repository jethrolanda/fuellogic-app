{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "fuellogic-app/introduction",
  "version": "0.1.0",
  "title": "FL Introduction",
  "category": "widgets",
  "attributes": {
    "question": {
      "type": "string"
    },
    "data": {
      "type": "array",
      "default": [
        ""
      ]
    },
    "correctAnswer": {
      "type": "number",
      "default": ""
    },
    "bgColor": {
      "type": "string",
      "default": "#EBEBEB"
    },
    "theAlignment": {
      "type": "string",
      "default": "left"
    }
  },
  "icon": "media-interactive",
  "description": "An interactive block with the Interactivity API",
  "example": {},
  "supports": {
    "interactivity": true,
    "align": true,
    "alignWide": true
  },
  "textdomain": "blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScriptModule": "file:./view.js"
}