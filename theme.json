{
  "settings": {
    "appearanceTools": true,
    "color": {
      "palette": [
        {
          "color": "#a2cd3a",
          "name": "Green",
          "slug": "custom-green"
        },
        {
          "color": "#878787",
          "name": "Gray",
          "slug": "custom-gray"
        },
        {
          "color": "#d9d9d9",
          "name": "Secondary Gray",
          "slug": "custom-secondary-gray"
        },
        {
          "color": "#1c1c1c",
          "name": "Darker",
          "slug": "custom-darker"
        },
        {
          "color": "#323030",
          "name": "Gray 2",
          "slug": "custom-gray-2"
        },
        {
          "color": "#ff8c57",
          "name": "orange",
          "slug": "custom-orange"
        }
      ]
    },
    "layout": {
      "contentSize": "620px",
      "wideSize": "1280px"
    },
    "spacing": {
      "units": [
        "%",
        "px",
        "em",
        "rem",
        "vh",
        "vw"
      ]
    },
    "typography": {
      "fontFamilies": [
        {
          "fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
          "name": "System Font",
          "slug": "system-font"
        },
        {
          "name": "Lato",
          "slug": "lato",
          "fontFamily": "Lato, sans-serif",
          "fontFace": [
            {
              "src": [
                "file:./assets/fonts/lato/lato-400-normal.woff2"
              ],
              "fontWeight": "400",
              "fontStyle": "normal",
              "fontFamily": "Lato"
            },
            {
              "src": [
                "file:./assets/fonts/lato/lato-700-normal.woff2"
              ],
              "fontWeight": "700",
              "fontStyle": "normal",
              "fontFamily": "Lato"
            },
            {
              "src": [
                "file:./assets/fonts/lato/lato-900-normal.woff2"
              ],
              "fontWeight": "900",
              "fontStyle": "normal",
              "fontFamily": "Lato"
            }
          ]
        }
      ]
    },
    "useRootPaddingAwareAlignments": true
  },
  "styles": {
    "blocks": {
      "core/button": {
        "border": {
          "radius": "6px"
        },
        "color": {
          "background": "var(--wp--preset--color--custom-green)",
          "text": "var(--wp--preset--color--black)"
        },
        "elements": {
          "link": {
            "color": {
              "text": "var(--wp--preset--color--black)"
            }
          }
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--system-font)",
          "fontStyle": "normal",
          "fontWeight": "600",
          "lineHeight": "1",
          "textTransform": "uppercase"
        }
      },
      "core/buttons": {
        "typography": {
          "lineHeight": "1"
        }
      }
    },
    "color": {
      "background": "var(--wp--preset--color--black)",
      "text": "var(--wp--preset--color--white)"
    },
    "elements": {
      "button": {
        "color": {
          "text": "var(--wp--preset--color--white)"
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--lato)"
        }
      },
      "caption": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--lato)"
        }
      },
      "heading": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--lato)"
        }
      },
      "link": {
        "color": {
          "text": "var(--wp--preset--color--white)"
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--lato)"
        }
      }
    },
    "spacing": {
      "blockGap": "1.2rem",
      "padding": {
        "bottom": "0px",
        "left": "var(--wp--preset--spacing--60)",
        "right": "var(--wp--preset--spacing--60)",
        "top": "0px"
      }
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--lato)"
    }
  },
  "templateParts": [
    {
      "area": "header",
      "name": "header"
    },
    {
      "area": "footer",
      "name": "footer"
    }
  ],
  "version": 3,
  "$schema": "https://schemas.wp.org/wp/6.6/theme.json"
}